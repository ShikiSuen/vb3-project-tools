</div>

<div data-role="content">
	<a href="projectpost.php?{vb:raw session.sessionurl}do=addreply&amp;issueid={vb:raw issue.issueid}" rel="nofollow" class="newreplylink" id="newreplylink" data-role="button" data-icon="plus" data-inline="true"><vb:if condition="$show['issue_closed']">{vb:rawphrase closed}<vb:else />{vb:rawphrase reply}</vb:if></a>

	<div id="postlist">
		{vb:raw pagenav}
		<ol id="posts" class="posts">
			{vb:raw issuefirstnote}

			<!-- notes -->
			<a name="notestart"></a>
			{vb:raw notebits}
			<!-- end notes -->
		</ol>
		{vb:raw pagenav}
	</div>

	<a href="projectpost.php?{vb:raw session.sessionurl}do=addreply&amp;issueid={vb:raw issue.issueid}" rel="nofollow" class="newreplylink" id="newreplylink" data-role="button" data-icon="plus" data-inline="true"><vb:if condition="$show['issue_closed']">{vb:rawphrase closed}<vb:else />{vb:rawphrase reply}</vb:if></a>

	<vb:if condition="$show['quick_reply'] OR $show['ajax_js']">
		<div id="qr_defaultcontainer" class="quickreplycontainer qr_defaultcontainer ui-body ui-body-c">
			<form class="vbform" name="quick_reply" id="quick_reply" method="post" action="newreply.php?do=postreply&amp;t={vb:raw threadid}">
				{vb:raw messagearea}

				<button type="submit" name="sbutton" data-theme="a">{vb:rawphrase post_quick_reply}</button>
				<fieldset class="ui-grid-a"> 
					<div class="ui-block-a"><button type="submit" name="preview" data-icon="plus">{vb:rawphrase go_advanced}</button></div>
					<div class="ui-block-b"><button type="button" name="cancel" data-theme="d">{vb:rawphrase cancel}</button></div>
				</fieldset>
		
				<input type="hidden" name="fromquickreply" value="1" />
				<input type="hidden" name="s" value="{vb:raw session.sessionhash}" />
				<input type="hidden" name="securitytoken" value="{vb:raw bbuserinfo.securitytoken}" />
				<input type="hidden" name="do" value="postreply" />
				<input type="hidden" name="issueid" value="{vb:raw issue.issueid}" id="qr_issueid" />
				<input type="hidden" name="issuenoteid" value="{vb:raw qrissuenoteid}" id="qr_issuenoteid" />
				<input type="hidden" name="specifiedissuenote" value="0" id="qr_specifiedissuenote" />
				<input type="hidden" name="loggedinuser" value="{vb:raw bbuserinfo.userid}" />
				<input type="hidden" name="posthash" value="" />
				<input type="hidden" name="poststarttime" value="" />
			</form>
		</div>
	</vb:if>
</div>

{vb:raw footer}

</body>
</html>

<vb:comment>
<div id="pt_issue_details">
