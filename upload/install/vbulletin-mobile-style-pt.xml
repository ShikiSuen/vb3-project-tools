<html xmlns="http://www.w3.org/1999/xhtml" dir="{vb:stylevar textdirection}" lang="{vb:stylevar languagecode}">
<head>
	{vb:raw headinclude}

	<title>{vb:raw vboptions.bbtitle} - {vb:rawphrase projects} - <vb:if condition="$issue['issueid']">{vb:rawphrase edit_issue}<vb:else />{vb:rawphrase post_new_issue}</vb:if></title>
	{vb:raw headinclude_bottom}
</head>
<body>

{vb:raw header}

{vb:raw navbar}

<div id="pagetitle" class="pagetitle ui-bar-b">
	<h1 class="pagetitle"><vb:if condition="$issue['issueid']">{vb:rawphrase edit_issue}<vb:else />{vb:rawphrase post_new_issue}</vb:if></h1>
	<p class="description">{vb:rawphrase issue_type}: {vb:raw issuetype_printable} | {vb:rawphrase project}: {vb:raw project.title_clean}</p>
</div>

<div data-role="content">
	<form class="vbform block" action="projectpost.php?{vb:raw session.sessionurl}do=postissue" method="post" name="vbform" onsubmit="return vB_Editor['{vb:raw editorid}'].prepare_submit(0, 0);">

<vb:comment>
	<vb:if condition="$issue['issueid']">
