		<div><span class="shade">{vb:rawphrase category}</span> {vb:raw issue.categorytitle}</div>
		<div><span class="shade">{vb:rawphrase status}</span> {vb:raw issue.status}</div>
		<div><span class="shade">{vb:rawphrase priority}</span> {vb:raw issue.priority_text}</div>

		<div><span class="shade">{vb:rawphrase applies_version_issuetype}</span> {vb:raw issue.appliesversion}</div>
		<div><span class="shade">{vb:rawphrase addressed_version_issuetype}</span> {vb:raw issue.addressedversion}</div>

		<vb:if condition="$show['milestone']">
			<div><span class="shade">{vb:rawphrase milestone}</span> {vb:raw issue.milestonetitle}</div>
		</vb:if>

		<div>
