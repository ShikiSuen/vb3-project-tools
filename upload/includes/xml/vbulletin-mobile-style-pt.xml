</div>

<div class="ui-content" data-role="content">
	<a href="projectpost.php?{vb:raw session.sessionurl}do=addissue&amp;projectid={vb:raw project.projectid}" rel="nofollow" id="newthreadlink_top" data-role="button" data-icon="plus" data-inline="true">{vb:rawphrase post_new_issue}</a>
	<div id="threadlist">
		<ul id="threads" class="threads ui-listview ui-grid-a" data-role="listview" role="listbox">
			<li class="ui-li ui-li-divider ui-bar-b" data-role="list-divider" role="heading" tabindex="0">
				{vb:rawphrase summary}
				<vb:if condition="$project['summary']">
					: {vb:raw project.summary}
				</vb:if>
			</li>
			<li class="threadbit dot ui-li" role="option" tabindex="-1" data-theme="c">{vb:raw project.description}</li>
		</ul>
	</div>

	<vb:if condition="$show['milestones']">
		<a href="projectmilestone.php?{vb:raw session.sessionurl}projectid={vb:raw project.projectid}" rel="nofollow" id="newthreadlink_bottom" data-role="button" data-icon="plus" data-inline="true">{vb:rawphrase view_milestones}</a>
	</vb:if>

	<a href="project.php?{vb:raw session.sessionurl}do=issuelist&amp;projectid={vb:raw project.projectid}" rel="nofollow" id="newthreadlink_bottom" data-role="button" data-icon="plus" data-inline="true">{vb:rawphrase view_complete_issue_list}</a>

	<vb:if condition="$issuebits">
