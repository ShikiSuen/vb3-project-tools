		<template name="pt_issuelist" templatetype="template" date="1315003634" username="Pitchoune" version="2.2.0"><![CDATA[{vb:stylevar htmldoctype}
<html xmlns="http://www.w3.org/1999/xhtml" dir="{vb:stylevar textdirection}" lang="{vb:stylevar languagecode}">
<head>
	{vb:raw headinclude}

	<title>{vb:raw project.title_clean} - {vb:rawphrase issue_list} - {vb:raw vboptions.bbtitle} - {vb:rawphrase projects}</title>

	{vb:raw headinclude_bottom}
</head>
<body>

{vb:raw header}
{vb:raw navbar}

<div id="pagetitle" class="pagetitle ui-bar-b">
	<h1 class="pagetitle">{vb:raw project.title_clean}</h1>
</div>

<div data-role="content">
	<a href="projectpost.php?{vb:raw session.sessionurl}do=addissue&amp;projectid={vb:raw project.projectid}" rel="nofollow" data-role="button" data-icon="plus" data-inline="true"> {vb:rawphrase post_new_issue}</a>

	<div id="threadlist">
	{vb:raw pagenav}
	<vb:if condition="$issuebits">
		<ul data-role="listview" id="threads" class="threads">
			<li data-role="list-divider">
				{vb:rawphrase issue_list}
			</li>
			{vb:raw issuebits}
		</ul>
	<vb:else />
		<div class="threadbit">
			<p class="nonthread">
				<strong>{vb:rawphrase no_issues_matching_criteria}</strong>
			</p>
		</div>
	</vb:if>
	{vb:raw pagenav}
	</div>

	<a href="projectpost.php?{vb:raw session.sessionurl}do=addissue&amp;projectid={vb:raw project.projectid}" rel="nofollow" data-role="button" data-icon="plus" data-inline="true"> {vb:rawphrase post_new_issue}</a>
</div>

{vb:raw footer}

</body>
</html>]]></template>
		<template name="pt_issuenotebit_user" templatetype="template" date="1315003406" username="Pitchoune" version="2.2.0"><![CDATA[<!-- user note -->
<li id="issuenote_{vb:raw note.issuenoteid}" class="block pt_issue_note">
	<div class="blockhead">
		<img src="{vb:stylevar imgdir_statusicon}/post_<vb:if condition="$note['newflag']">new<vb:else />old</vb:if>.png" alt="" class="inlineimg" hspace="3" vspace="3" /> {vb:raw note.note_date} {vb:raw note.note_time} <a name="note{vb:raw note.issuenoteid}"></a>
	</div>
	<div class="blockbody settings_form_border">
		<div class="blockrow">
			<div class="floatcontainer">
				<vb:if condition="$show['avatar']"><a href="{vb:link member, {vb:raw note}}"><img class="author_avatar" src="{vb:raw note.avatarurl}" {vb:raw note.avwidth} {vb:raw note.avheight} alt="{vb:rawphrase xs_avatar, {vb:raw note.username}}" border="0" /></a></vb:if>

				<div id="notemenu_{vb:raw note.issuenoteid}">
					<vb:if condition="$show['profile']">
						<a class="bigtext" href="{vb:link member, {vb:raw note}}">{vb:raw note.musername}</a>
						<vb:if condition="$show['onlinestatus']">
							<vb:if condition="$note['onlinestatus'] == 0">
								<img class="inlineimg onlinestatus" src="{vb:stylevar imgdir_statusicon}/user-offline.png" alt="{vb:rawphrase x_is_offline, {vb:raw note.username}}" border="0" />
							<vb:elseif condition="$note['onlinestatus'] == 1" />
								<img class="inlineimg onlinestatus" src="{vb:stylevar imgdir_statusicon}/user-online.png" alt="{vb:rawphrase x_is_online_now, {vb:raw note.username}}" border="0" />
							<vb:elseif condition="$note['onlinestatus'] == 2" />
								<img class="inlineimg onlinestatus" src="{vb:stylevar imgdir_statusicon}/user-invisible.png" alt="{vb:rawphrase x_is_invisible, {vb:raw note.username}}" border="0" />
							</vb:if>
						</vb:if>
					<vb:else />
						{vb:raw note.musername}
					</vb:if>
				</div>
				<vb:if condition="$note['usertitle']"><div class="smallfont">{vb:raw note.usertitle}</div></vb:if>

				<vb:if condition="$note['import_issueid']"><div class="smallfont"><a href="{vb:raw note.import_seo}">{vb:rawphrase issuenote_made_as_issue}</a></div></vb:if>
			</div>
		</div>
		<div class="blockrow issuetext">
			<div class="issue_message" id="issuenote_message_{vb:raw note.issuenoteid}">
				<div class="restore">{vb:raw note.message}</div>

				<vb:if condition="$note['visible'] == 'private'">
					<div class="smallfont" style="margin-top: 10px"><img src="{vb:stylevar imgdir_misc}/issue_private.png" class="inlineimg" alt="" /> {vb:rawphrase private_message_keep_in_mind}</div>
				</vb:if>

				<vb:if condition="$note['visible'] == 'deleted'">
					<div class="smallfont" style="margin-top: 10px">
						<vb:if condition="$show['edit_note']">
							<div style="float:{vb:stylevar right}">
								<a href="projectpost.php?{vb:raw session.sessionurl}do=editreply&amp;issuenoteid={vb:raw note.issuenoteid}">{vb:rawphrase undelete_ellipsis}</a>
							</div>
						</vb:if>
						<img src="{vb:stylevar imgdir_misc}/trashcan_small.gif" class="inlineimg" alt="" /> {vb:rawphrase message_has_been_deleted}
					</div>
				</vb:if>
			</div>

			<div class="issuefoot">
				<div class="textcontrols">
					<span class="issuecontrols">

					<img style="margin: 0; padding: 0;" class="hidden" id="issuenote_progress_{vb:raw note.issuenoteid}" src="images/misc/progress.gif" alt="" />

					<vb:if condition="$show['edit_note']">
						<a class="editissue" href="projectpost.php?{vb:raw session.sessionurl}do=editreply&amp;issuenoteid={vb:raw note.issuenoteid}" id="issuenote_edit_{vb:raw note.issuenoteid}"><img alt="{vb:rawphrase edit}" src="clear.gif" title="{vb:rawphrase edit}" /> {vb:rawphrase edit}</a>
					</vb:if>

					<vb:if condition="$show['reply_note'] AND $show['edit_note']">
						<span class="separator">&nbsp;</span>
					</vb:if>

					<vb:if condition="$show['reply_note']">
						<a class="replyissue" href="projectpost.php?{vb:raw session.sessionurl}do=addreply&amp;issueid={vb:raw issue.issueid}&amp;quotenoteid={vb:raw note.issuenoteid}"><img alt="{vb:rawphrase reply}" src="clear.gif" title="{vb:rawphrase reply}" /> {vb:rawphrase reply}</a>
					</vb:if>

					<vb:if condition="$show['export_note']">
						<span class="separator">&nbsp;</span>
						<a class="replyissue" href="projectpost.php?{vb:raw session.sessionurl}do=export&amp;issuenoteid={vb:raw note.issuenoteid}"><img alt="{vb:rawphrase export}" src="clear.gif" title="{vb:rawphrase export}" /> {vb:rawphrase export}</a>
					</vb:if>
					</span>

					<span class="issuelinking">
						<vb:if condition="$show['edit_history']">
							<a class="issuehistory" href="issue.php?{vb:raw session.sessionurl}do=notehistory&amp;issuenoteid={vb:raw note.issuenoteid}">{vb:rawphrase last_updated_x_at_y, {vb:raw note.lastedit_date}, {vb:raw note.lastedit_time}}</a>
						</vb:if>

						<vb:if condition="$show['edit_history'] AND ($show['reportlink'] OR $issue['noteipaddress'] OR $show['import_note'])">
							<span class="separator">&nbsp;</span>
						</vb:if>

						<vb:if condition="$show['import_note']">
							<a href="projectpost.php?{vb:raw session.sessionurl}do=importcontent&amp;issuenote={vb:raw note.issuenoteid}&amp;type=issuenote" class="promoteissue">{vb:rawphrase promote_to_ptissue}</a>
						</vb:if>

						<vb:if condition="($show['edit_history'] OR $show['import_note']) AND ($show['reportlink'] OR $issue['noteipaddress'])">
							<span class="separator">&nbsp;</span>
						</vb:if>

						<vb:if condition="$show['reportlink']">
							<a class="reportissue" href="issue.php?{vb:raw session.sessionurl}do=report&amp;issuenoteid={vb:raw note.issuenoteid}" rel="nofollow" title="{vb:rawphrase report_issue_note}">&nbsp;</a>
						</vb:if>
						<vb:if condition="$issue['noteipaddress']">
							<a class="ipissue" href="issue.php?do=viewip&amp;issuenoteid={vb:raw note.issuenoteid}" title="{vb:raw note.noteipaddress}">&nbsp;</a>
						</vb:if>
					</span>
				</div>
			</div>
		</div>
	</div>
</li>]]></template>
